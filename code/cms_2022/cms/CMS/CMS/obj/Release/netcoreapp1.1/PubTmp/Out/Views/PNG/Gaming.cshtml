                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Weekly.Won.ToString("N")</td>
                                <td>
                                    @if (Model.All.Weekly.Bet > 0)
                                    {@Math.Round(((Model.All.Weekly.Bet - Model.All.Weekly.Won) / Model.All.Weekly.Bet) * 100, 2);
                                }%
                                </td>
                            </tr>
                            <tr>
                                <td>Last 30 Days</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Monthly.Deposited.ToString("N")</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Monthly.DepositedBonus.ToString("N")</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Monthly.Withdrawn.ToString("N")</td>
                                <td>@Model.All.Monthly.Bets.ToString("N0")</td>
