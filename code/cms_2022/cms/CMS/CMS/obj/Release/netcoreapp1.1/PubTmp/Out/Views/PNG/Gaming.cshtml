@using Microsoft.AspNetCore.Http;

@model GamingViews
@{
    ViewData["Title"] = "Gaming Activity | Console";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12 page-header">
            <h1>
                Gaming Activity
                <span class="float-right">
                </span>
            </h1>
        </div>
    </div>
    <div class="row">
        <div class="col-12 pl-0">
            <div class="panel">
                <div class="panel-heading">
                    <div class="title">
                        <span class="sr-only">Loading...</span>
                        All Activity<i class="fas fa-spinner" aria-hidden="true"></i>
                    </div>
                    <span class="text-success">@TempData["SuccessMessage"]</span>
                    <div class="float-right btn-toolbar">
                    </div>
                </div>
                <div class="panel-body resultspanel m-0 p-0">
                    <table class="table table-hover table-striped m-0" id="results-table">
                        <thead>
                            <tr>
                                <th>Period</th>
                                <th class="tw-medium">Deposited</th>
                                <th class="tw-medium">Bonuses Awarded</th>
                                <th class="tw-medium">Withdrawn</th>
                                <th class="tw-medium">Bets Placed</th>
                                <th class="tw-medium">Turnover</th>
                                <th class="tw-medium">Paid Out</th>
                                <th class="tw-medium">% Kept</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Last 24 Hours</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Daily.Deposited.ToString("N")</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Daily.DepositedBonus.ToString("N")</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Daily.Withdrawn.ToString("N")</td>
                                <td>@Model.All.Daily.Bets.ToString("N0")</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Daily.Bet.ToString("N")</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Daily.Won.ToString("N")</td>
                                <td>
                                    @if (Model.All.Daily.Bet > 0)
                                    {@Math.Round(((Model.All.Daily.Bet - Model.All.Daily.Won) / Model.All.Daily.Bet) * 100, 2);
                                }%
                                </td>
                            </tr>
                            <tr>
                                <td>Last 7 Days</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Weekly.Deposited.ToString("N")</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Weekly.DepositedBonus.ToString("N")</td>
                                <td>@Context.Session.GetString("CurrencySymbol")@Model.All.Weekly.Withdrawn.ToString("N")</td>
                                <td>@Model.All.Weekly.Bets.ToString("N0")</td>
